# Project: CPPJSON
# Makefile created by Red Panda C++ 3.0.2815

CXX      = "g++.exe"
CC       = "gcc.exe"
WINDRES  = "windres.exe"
RM       = del /q /f
CD       = cd /d
LIBS     = "-Wl,--stack,20971520" "-s" "-static"
INCS     = 
CXXINCS  = 
CXXFLAGS = $(CXXINCS) "-O3" "-std=c++2b" "-pipe"
CFLAGS   = $(INCS) "-std=c17" "-O3" "-pipe"
WINDRESFLAGS = 
RES      = CPPJSON_private.res
OBJ      = main.o JSON_ACC_Pool.o JSON_Parse.o $(RES)
BIN      = CPPJSON.exe
LINKOBJ  = "main.o" "JSON_ACC_Pool.o" "JSON_Parse.o" "CPPJSON_private.res"
CLEANOBJ = "main.o" "JSON_ACC_Pool.o" "JSON_Parse.o" "CPPJSON_private.res" "CPPJSON.exe"

.PHONY: all all-before all-after clean clean-custom

all: all-before $(BIN) all-after

clean: clean-custom
	-$(RM) $(CLEANOBJ) >NUL 2>&1

CPPJSON.exe: $(OBJ)

	$(CXX) $(LINKOBJ) -o "CPPJSON.exe" $(LIBS)

main.o: main.cpp JSON_Base.h JSON_IO.h
	$(CXX) -c "main.cpp" -o "main.o" $(CXXFLAGS) 

JSON_ACC_Pool.o: JSON_ACC_Pool.cxx JSON_Base.h
	$(CXX) -c "JSON_ACC_Pool.cxx" -o "JSON_ACC_Pool.o" $(CXXFLAGS) 

JSON_Parse.o: JSON_Parse.cxx JSON_Base.h
	$(CXX) -c "JSON_Parse.cxx" -o "JSON_Parse.o" $(CXXFLAGS) 

CPPJSON_private.res: CPPJSON_private.rc 
	$(WINDRES) -i "CPPJSON_private.rc" --input-format=rc -o "CPPJSON_private.res" -O coff $(WINDRESFLAGS) 

